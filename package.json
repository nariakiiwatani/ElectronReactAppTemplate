{
  "name": "ElectronReactAppTemplate",
  "version": "0.1.0",
  "main": "src/electron-starter.js",
  "homepage": "./",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
